<Application x:Class="TOZAN_Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TOZAN_Client"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Designs.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="SpicaNeueP-Regular">/Assets/SpicaNeueP-Regular.ttf#Spica Neue P</FontFamily>
            <FontFamily x:Key="SpicaNeueP-Light">/Assets/SpicaNeueP-Light.ttf#Spica Neue P Light</FontFamily>
            <FontFamily x:Key="SpicaNeueP-Bold">/Assets/SpicaNeueP-Bold.ttf#Spica Neue P Bold</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
